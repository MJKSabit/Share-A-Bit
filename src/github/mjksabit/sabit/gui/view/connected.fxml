<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXProgressBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="300.0" style="-fx-background-color: #e9f5fb;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="github.mjksabit.sabit.gui.controller.Connected">
   <children>
      <Pane fx:id="topPane" layoutY="-1.0" prefHeight="47.0" prefWidth="300.0" style="-fx-background-color: #2c9dd7;">
         <children>
            <Label fx:id="otherUser" layoutX="18.0" layoutY="2.0" prefHeight="40.0" prefWidth="181.0" text="Other User" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="30.0" />
               </font>
            </Label>
            <JFXButton layoutX="242.0" layoutY="6.0" onAction="#endShare" prefHeight="36.0" prefWidth="53.0" style="-fx-background-color: #ff4d4d;" text="End" textFill="WHITE" />
         </children>
      </Pane>
      <JFXButton buttonType="RAISED" contentDisplay="GRAPHIC_ONLY" graphicTextGap="0.0" layoutX="201.0" layoutY="458.0" onAction="#addFolder" style="-fx-background-color: #33cc33;" text="+ Folder">
         <graphic>
            <ImageView fitHeight="26.0" fitWidth="26.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/add-folder.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <JFXButton buttonType="RAISED" contentDisplay="GRAPHIC_ONLY" graphicTextGap="0.0" layoutX="251.0" layoutY="459.0" onAction="#addFile" style="-fx-background-color: #33cc33;" text="+ Folder">
         <graphic>
            <ImageView fitHeight="25.0" fitWidth="26.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/add-file.png" />
               </image>
            </ImageView>
         </graphic>
      </JFXButton>
      <ListView fx:id="transferFileList" layoutY="200.0" prefHeight="251.0" prefWidth="300.0" />
      <Label layoutX="14.0" layoutY="173.0" text="Transfers">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <JFXButton buttonType="RAISED" layoutX="8.0" layoutY="458.0" onAction="#cancelSendingAll" prefHeight="36.0" prefWidth="165.0" style="-fx-background-color: #ff4d4d;" text="Cancel Sending" textFill="WHITE" />
      <Label fx:id="speedText" alignment="CENTER_RIGHT" layoutX="150.0" layoutY="169.0" prefHeight="29.0" prefWidth="104.0" text="2.251">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
         <graphic>
            <ImageView fitHeight="22.0" fitWidth="22.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/up-down.png" />
               </image>
            </ImageView>
         </graphic>
      </Label>
      <Label layoutX="257.0" layoutY="177.0" text="MB/s" />
      <Pane fx:id="sendPane" layoutX="10.0" layoutY="51.0" prefHeight="55.0" prefWidth="280.0" style="-fx-background-color: #00000020; -fx-background-radius: 5px;" visible="false">
         <children>
            <Label layoutX="7.0" layoutY="3.0" text="Sending:" />
            <Label fx:id="sendFileNameText" layoutX="78.0" layoutY="3.0" prefHeight="18.0" prefWidth="202.0" text="FileName.ext" />
            <JFXProgressBar fx:id="sendProgressBar" layoutX="7.0" layoutY="26.0" prefHeight="3.0" prefWidth="181.0" />
            <Label fx:id="sentInMB" layoutX="7.0" layoutY="32.0" text="8888.888 MB" />
            <Label fx:id="sendTotalInMB" alignment="CENTER_RIGHT" layoutX="104.0" layoutY="32.0" prefHeight="18.0" prefWidth="87.0" text="8888.888 MB" />
            <Label layoutX="95.0" layoutY="32.0" text="/" />
            <Label fx:id="sentPercentage" alignment="CENTER_RIGHT" layoutX="204.0" layoutY="22.0" prefHeight="29.0" prefWidth="61.0" text="100%">
               <font>
                  <Font name="System Bold" size="17.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Pane fx:id="receivePane" layoutX="10.0" layoutY="109.0" prefHeight="55.0" prefWidth="280.0" style="-fx-background-color: #00000020; -fx-background-radius: 5px;" visible="false">
         <children>
            <Label layoutX="7.0" layoutY="3.0" text="Receiving:" />
            <Label fx:id="receiveFileNameText" layoutX="78.0" layoutY="3.0" prefHeight="18.0" prefWidth="202.0" text="FileName.ext" />
            <JFXProgressBar fx:id="receiveProgressBar" layoutX="7.0" layoutY="26.0" prefHeight="3.0" prefWidth="181.0" />
            <Label fx:id="receivedInMB" layoutX="7.0" layoutY="32.0" text="8888.888 MB" />
            <Label fx:id="receivedTotalInMB" alignment="CENTER_RIGHT" layoutX="104.0" layoutY="32.0" prefHeight="18.0" prefWidth="87.0" text="8888.888 MB" />
            <Label layoutX="95.0" layoutY="32.0" text="/" />
            <Label fx:id="receivedPercentage" alignment="CENTER_RIGHT" layoutX="204.0" layoutY="22.0" prefHeight="29.0" prefWidth="61.0" text="100%">
               <font>
                  <Font name="System Bold" size="17.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</AnchorPane>
